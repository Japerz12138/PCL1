<UserControl x:Class="Selecter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="155" d:DesignWidth="46" Name="panBack" FontSize="14" Foreground="{DynamicResource ResourceKey=Color5}">
    <Grid>
        <Rectangle Height="24" HorizontalAlignment="Left" Name="lineSelect" UseLayoutRounding="True" Width="5" VerticalAlignment="Top" Stroke="{Binding ElementName=panBack, Path=Foreground}" StrokeThickness="3" Margin="0,4,0,0" />
        <StackPanel Name="panLabs" Margin="14,0,0,0"></StackPanel>
    </Grid>
</UserControl>
